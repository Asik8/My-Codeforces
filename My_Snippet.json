	"prefix": "cdivisor",
	"body": [
	  "vector<ll>divisors(ll n){",
	  "    vector<ll>v;",
	  "    for(ll i=1;i*i<=n;i++){",
	  "        if(!(n%i)){",
	  "            v.pb(i);",
	  "            if((n/i) != i) v.pb(n/i);",
	  "        }",
	  "    }",
	  "    sort(v.begin(),v.end());",
	  "    return v;",
	  "}"
